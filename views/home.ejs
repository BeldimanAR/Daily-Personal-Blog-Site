
<%- include('partials/header'); -%>

<h1 name = "home">Home</h1>
<p><%=startingContent %></p>

<%postArray.forEach((post) => {%>
  <%let addTitle = post.title;%>
  <%let addContent = post.content.substring(0, 100);%>
  <h1><%=addTitle%></h1>
  <p><%=addContent%> ...
  <a href="/posts/<%=parameter(addTitle)%>">Read More</a></p>

<%})%>

<%- include('partials/footer'); -%>
